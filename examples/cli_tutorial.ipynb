    "!catplat calculate -p wgs --chemsys Cu --e-above-hull \"0\" --miller-index 2 1 1 --unitcell-size 1 3 --num-layers 12 \\\n",
    "--num-fixed-layers 6 --adsorbate-atoms O2  --bonds \"[0,1]\" --avg-coord-num \"['<=8','<=8']\" --test"
